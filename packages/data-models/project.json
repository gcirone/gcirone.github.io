{
  "name": "data-models",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/data-models/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/data-models",
        "main": "packages/data-models/src/index.ts",
        "tsConfig": "packages/data-models/tsconfig.lib.json",
        "assets": ["packages/data-models/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/data-models/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/data-models/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "db-push": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma db push --schema ./src/prisma/schema.prisma",
        "cwd": "packages/data-models"
      }
    },
    "db-seed": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma db seed",
        "cwd": "packages/data-models"
      }
    },
    "migrate-dev": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma migrate dev --schema ./src/prisma/schema.prisma",
        "cwd": "packages/data-models"
      }
    },
    "generate-client": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma generate --schema ./src/prisma/schema.prisma",
        "cwd": "packages/data-models"
      }
    }
  },
  "tags": []
}
